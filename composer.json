{
    "name": "laraxot/module_activity_fila3",
    "description": "",
    "homepage": "https://github.com/laraxot/module_activity_fila3",
    "license": "MIT",
    "authors": [{
        "name": "Marco Sottana",
        "email": "marco.sottana@gmail.com"
    }],
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\Activity\\Providers\\ActivityServiceProvider"
            ],
            "aliases": {}
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\Activity\\": ""
        }
    },
    "require": {
        "illuminate/contracts": "^9.0|^10.0",
        "illuminate/support": "^9.12|^10.0",
        "laraxot/module_xot_fila3": "dev-master",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-event-sourcing": "^7.3",
        "thecodingmachine/safe": "^2.5"
    },
    "require-dev": {
        "driftingly/rector-laravel": "^0.26.2",
        "enlightn/enlightn": "^2.7",
        "larastan/larastan": "^2.7",
        "laravel/pint": "^1.13",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/phpinsights": "^2.11",
        "orchestra/pest-plugin-testbench": "^2.0",
        "orchestra/testbench": "^8.22",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "psalm/plugin-laravel": "^2.8",
        "rector/rector": "^0.18.12",
        "spatie/laravel-ray": "^1.26",
        "thecodingmachine/phpstan-safe-rule": "^1.2",
        "vimeo/psalm": "^5.17"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../Xot"
        },
        {
            "type": "path",
            "url": "../Tenant"
        },
        {
            "type": "path",
            "url": "../UI"
        }
    ],
    "scripts": {
        "post-autoload-dump": [
            "@php vendor/bin/testbench package:discover --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/pest --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "_comment_vendor-dir": "../../vendor",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
